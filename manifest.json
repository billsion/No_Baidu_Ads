{
  "name": "Baidu Ads Filter",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs", "http://*/*"
  ],
  "browser_action": {
      "name": "Click to switch the Ads filter.",
      "default_icon": {
        "19": "disabled.png"
      },
      "default_title": "disable"
  },
  "manifest_version": 2
}
